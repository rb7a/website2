{
  "name": "basement",
  "version": 2,
  "builds": [{ "src": "next.config.js", "use": "@now/next" }],
  "routes": [
    { "src": "/blog/(?<slug>[^/]+)$", "dest": "/blog?slug=$slug" },
    { "src": "/sitemap.xml", "dest": "/static/sitemap.xml" },
    { "src": "/robots.txt", "dest": "/static/robots.txt" },
    { "src": "/favicon.ico", "dest": "/static/favicon.ico" }
  ]
}
